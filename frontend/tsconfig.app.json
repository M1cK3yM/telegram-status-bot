{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@shared/*": [
        "../shared/src/*"
      ]
    },
    "tsBuildInfoFile": "../tmp/tsconfig.app.tsbuildinfo",
    "useDefineForClassFields": true,
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
